{
  "name": "gatsby-plugin-stylable",
  "version": "1.1.2",
  "description": "Gatsby plugin for enabling wix/stylable support.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir .",
    "lint": "eslint 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*'",
    "prepublish": "npm run prepare"
  },
  "keywords": [
    "gatsby-plugin",
    "gatsby",
    "plugin",
    "wix",
    "stylable"
  ],
  "author": "Aaron Ross <superhawk610@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/superhawk610/gatsby-plugin-stylable.git"
  },
  "license": "ISC",
  "dependencies": {
    "@stylable/webpack-plugin": "^2.0.13"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.17.1"
  }
}
